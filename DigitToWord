class Point {
    int x, y;
    Point(int x, int y) {
      this.x = x;
      this.y = y;
    }
}
new Point(1, 2).equals(new Point(1,2))



class Foo {
    int count = 0;
    void increment() {
        count = count + 1;
    }
    void run() {
        ExecutorService executor = Executors.newFixedThreadPool(2);
        IntStream.range(0, 10000)
            .forEach(i -> executor.submit(this::increment));
        // stop(executor);
        System.out.println(count);
    }
}



